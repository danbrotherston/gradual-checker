#!/bin/sh

if [ -z $TESTSRC ]
    then
      TESTSRC=.
fi

if [ -z $TESTCLASSES ]
    then
      TESTCLASSES=.
fi

$TESTSRC/../../bin/javac -d $TESTCLASSES -processor org.checkerframework.checker.gradualnullness.GradualNullnessChecker $TESTSRC/MethodSuperTest.java $TESTSRC/MethodSuperTestParent.java
javap $TESTCLASSES/MethodSuperTest.class > $TESTCLASSES/MethodSuperTest.testout
diff $TESTCLASSES/MethodSuperTest.testout $TESTSRC/MethodSuperTest.out
